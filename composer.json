{
	"name": "arad/bot-blocker",
	"homepage": "https://git.jeyserver.com/arad/bot-blocker",
	"require": {
		"php": "^7.4|^8.0",
		"dnj/local-filesystem": "^1.0",
		"dnj/log": "dev-master",
		"kassner/log-parser": "^2.1",
		"league/mime-type-detection": "^1.9",
		"krowinski/php-inotify": "^2.0",
		"illuminate/container": "5.4"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"phpstan/phpstan": "^1.4.1",
		"friendsofphp/php-cs-fixer": "^3.1"
	},
	"scripts": {
		"test:types": "vendor/bin/phpstan analyze",
		"test:phpunit": "vendor/bin/phpunit",
		"test:codestyle": "vendor/bin/php-cs-fixer fix -v --dry-run --stop-on-violation --using-cache=no",
		"test": [
			"@test:types",
			"@test:phpunit",
			"@test:codestyle"
		]
	},
	"autoload": {
		"psr-4": {
			"Arad\\BotBlocker\\": "src/"
		}
	},
	"bin": [
		"bin/bot-blocker"
	],
	"repositories": [
		{
			"type": "path",
			"url": "/home/composer/dnj/log"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true
}
